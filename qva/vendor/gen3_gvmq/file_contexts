# Dev nodes
#
/dev/spidev2.0                                                                             u:object_r:audio_device:s0

#GNSS device
/dev/ttyHS1                                                                                u:object_r:location_device:s0
/dev/gnss_sirf                                                                             u:object_r:location_device:s0

/vendor/lib(64)?/libuhab\.so                                                               u:object_r:same_process_hal_file:s0

/(vendor|system/vendor)/bin/hw/vendor.qti.hardware.automotive.vehicle@1.0-service          u:object_r:hal_automotive_vehicle_qti_exec:s0

/(vendor|system/vendor)/bin/hw/vendor.qti.hardware.automotive.audiocontrol@1.0-service     u:object_r:hal_audiocontrol_qti_exec:s0
/(vendor|system/vendor)/bin/hyp_dvr_mux_be                                                 u:object_r:hyp_dvr_mux_be_exec:s0
/(vendor|system/vendor)/bin/hw/vendor\.qti\.dvr@1\.0-service                               u:object_r:hal_dvr_qti_exec:s0

/sys/devices/platform/soc/a600000.ssusb/mode            u:object_r:sysfs_usb_mode:s0
/sys/devices/platform/soc/a800000.ssusb/mode            u:object_r:sysfs_usb_mode:s0

# Car power daemon
/(vendor|system/vendor)/bin/car_power_daemon            u:object_r:carpowerd_exec:s0

/(vendor|system/vendor)/bin/audiod                      u:object_r:audiod_exec:s0

# amfs service
/(vendor|system/vendor)/bin/amfsservice                             u:object_r:amfsservice_exec:s0

# EVS and Qcarcam HAL
/(vendor|system/vendor)/bin/android\.hardware\.automotive\.evs@1\.0-ais     u:object_r:hal_evs_driver_exec:s0
/(vendor|system/vendor)/bin/hw/vendor\.qti\.automotive\.qcarcam@1\.0-service     u:object_r:hal_ais_qti_exec:s0

#Old partitions
/dev/block/vda                                                      u:object_r:system_block_device:s0
/dev/block/vdb                                                      u:object_r:userdata_block_device:s0
/dev/block/vdc                                                      u:object_r:system_block_device:s0
/dev/block/vdd                                                      u:object_r:persist_block_device:s0
/dev/block/vde                                                      u:object_r:modem_block_device:s0
/dev/block/vdf                                                      u:object_r:modem_block_device:s0
/dev/block/vdg                                                      u:object_r:misc_block_device:s0

#New partitions
/dev/block/la_system                                                u:object_r:system_block_device:s0
/dev/block/la_userdata                                              u:object_r:userdata_block_device:s0
/dev/block/la_vendor                                                u:object_r:system_block_device:s0
/dev/block/la_persist                                               u:object_r:persist_block_device:s0
/dev/block/modem                                                    u:object_r:modem_block_device:s0
/dev/block/bluetooth                                                u:object_r:modem_block_device:s0
/dev/block/la_misc                                                  u:object_r:misc_block_device:s0

/dev/ttyAMA0                                                        u:object_r:tty_device:s0
/dev/hab                                                            u:object_r:hab_device:s0

#SW Sync
/sys/kernel/debug/sync/sw_sync                                      u:object_r:sw_sync_file:s0

#####################################
# DVR
/mnt/vendor/dvr_ctl(/.*)?                                                  u:object_r:dvr_pipe_file:s0
/mnt/vendor/dvr_mux(/.*)?                                                  u:object_r:dvr_mux_file:s0

#ssr_setup can set ssr restart_level
/sys/devices/platform/soc/soc:qcom,cnss-qca-converged/subsys[0-9]+/name                u:object_r:sysfs_ssr:s0
/sys/devices/platform/soc/soc:qcom,cnss-qca-converged/subsys[0-9]+/restart_level       u:object_r:sysfs_ssr_toggle:s0
